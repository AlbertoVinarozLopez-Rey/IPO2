<Window x:Class="MiAppVenom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiAppVenom"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <Window.Resources>
        <Style x:Key="estiloBorde" TargetType="Button">
            <Setter Property="OverridesDefaultStyle" Value="True"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Name="border" 
                            BorderBrush="White" 
                            Background="{TemplateBinding Background}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="BorderBrush" Value="White" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="White" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <DockPanel LastChildFill="False">
            <StackPanel Background="#FF252129" Height="714" Width="118.182" Margin="0,60.121,0,0">
                <Button x:Name="BEnergia" Style="{StaticResource estiloBorde}" Height="97"  Click="BEnergia_Click" Width="100" Margin="10,10,8.182,10">
                    <Button.Foreground>
                        <ImageBrush ImageSource="venom-energia.jpg" Stretch="UniformToFill"/>
                    </Button.Foreground>
                    <Button.Background>
                        <ImageBrush ImageSource="venom-energia.jpg" Stretch="UniformToFill"/>
                    </Button.Background> Dormir
                </Button>
                <Button x:Name="BApetito" Style="{StaticResource estiloBorde}" Height="97"  Click="BApetito_Click" Width="100" Margin="10,10,8.182,10">
                    <Button.Background>
                        <ImageBrush ImageSource="venom-apetito.jpg" Stretch="UniformToFill"/>
                    </Button.Background> Comer
                </Button>
                <Button x:Name="BDiversion" Style="{StaticResource estiloBorde}" Height="97"  Click="BDiversion_Click"  Width="100" Margin="10,10,8.182,10">
                    <Button.Background>
                        <ImageBrush ImageSource="venom-diversion.jpg" Stretch="UniformToFill"/>
                    </Button.Background> Jugar
                </Button>
            </StackPanel>

        </DockPanel>
        <Label  Content="APP VENOM" HorizontalAlignment="Left" VerticalAlignment="Top" Height="38" Width="1016"  Margin="0,16,0,0" Foreground="White"/>
        <Button Content="Notificaciones"  HorizontalAlignment="Right" VerticalAlignment="Top" Height="38" Width="167" Margin="0,10,10,0"></Button>
        <Grid Margin="118.182,59,0,0" >
            <Grid.Background>
